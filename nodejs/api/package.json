{
  "name": "otus-randomization",
  "version": "1.0.0",
  "description": "Node api for randomization",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "dev": "node --max-old-space-size=1024 --optimize-for-size app.js",
    "unit": "mocha test/**/*.js",
    "gulp": "gulp",
    "start": "npm run gulp browser-sync"
  },
  "nyc": {
    "include": [
      "app/**/*.js",
      "test/**/*-spec.js"
    ],
    "extension": [
      ".js"
    ],
    "all": true,
    "require": [],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "consign": "0.1.6",
    "express": "4.16.4",
    "express-list-endpoints": "4.0.1",
    "mongoose": "5.3.8"
  },
  "devDependencies": {
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "gulp": "3.9.1",
    "browser-sync": "2.11.2"
  }
}
